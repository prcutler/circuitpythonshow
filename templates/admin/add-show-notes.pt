            Link 8: <input name="link_8" type="text" placeholder="Link 8" class="form-control" value="${ link_8 }" >
            Link 8 Text: <input name="link_text_8" type="text" placeholder="Link 8 Text" class="form-control" value="${ link_text_8 }" >
            <br />

            Timestamp 9: <input name="timestamp_9" type="text" placeholder="Timestamp 9" class="form-control" value="${ timestamp_9 }" >
            Notes 9: <input name="notes_9" type="text" placeholder="Notes 9" class="form-control" value="${ notes_9 }" >
            Link 9: <input name="link_9" type="text" placeholder="Link 9" class="form-control" value="${ link_9 }" >
            Link 9 Text: <input name="link_text_9" type="text" placeholder="Link 9 Text" class="form-control" value="${ link_text_9 }" >
            <br />

            Timestamp 10: <input name="timestamp_10" type="text" placeholder="Timestamp 10" class="form-control" value="${ timestamp_10 }" >
            Notes 10: <input name="notes_10" type="text" placeholder="Notes 10" class="form-control" value="${ notes_10 }" >
