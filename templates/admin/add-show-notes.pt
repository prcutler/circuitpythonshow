            Link 10 Text: <input name="link_text_10" type="text" placeholder="Link 10 Text" class="form-control" value="${ link_text_10 }" >


        

            <br />
            <button type="submit" class="btn btn-danger">Add Show Notes</button>
            <br />

            <br />
                <div style="clear: both;"></div>
                <br /><br /><br />

                <div class="error-msg" tal:condition="error">${ error }</div>
            </form>

        </div>
    </div>
    </div>

    <div metal:fill-slot="additional_css" tal:omit-tag="True">
        <link rel="stylesheet" href="/static/css/account.css">
    </div>
</div>